    "html-webpack-plugin": "^5.5.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "prettier": "^1.17.1",
    "prettier-quick": "0.0.5",
    "pretty-quick": "^1.11.0",
